<div class="container">
  <div class="classroom">
    <h1 class="title">welcome
      <span class="username">
       <% if @username %>
         <%= @username %>
       <% end %>
      </span>
    </h1>
    <ul class="participants"></ul>
  </div>
  <div class="chatroom">
    <ul class="messages"></ul>
    <form action="">
      <input class="msg" autocomplete="off" /><button>Send</button>
    </form>
  </div>
</div>

<%= javascript_include_tag "real-time-chat" %>
